{
  "expo": {
    "name": "krasgid",
    "slug": "krasgid",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app-icon.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.xiely.krasgid"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/app-icon.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "263d0460-02d9-459b-8acd-dd896e702a8d"
      }
    }
  }
}
